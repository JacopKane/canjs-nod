	"keywords"		: [
		"canjs",
		"can",
		"mvc",
		"requirejs",
		"require",
		"less",
		"css",
		"mustache"
	],
	"authors"		: [
		"Furkan TunalÄ± <furkan@nod.st>",
		"NOD Studios <hey@nod.st>"
	],
    "ignore": [
